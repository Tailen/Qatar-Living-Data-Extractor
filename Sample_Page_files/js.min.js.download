var Document = Backbone.View.extend({
  el: document,

  initialize: function() {
    this.render();
  },

  render: function() {
    this.presentationSlider();
    this.firstCommentBlock();
    this.commentFileUploader();
    //this.googleMap();
    this.labelState();
    this.select();
  },

  events: {
    'click .b-presentation-slider--el-directions': 'slideDirections',
    'click .js-comment-tab-control': 'toggleComments',
    'focus .js-focus-add-comment-field': 'addCommentFieldActivated',
    'click .b-header--el-menu-button': 'openSideMenu',
    'click .b-side-menu--el-close': 'closeSideMenu',
//    'click .b-options-block .b-options-block--el-label': 'checkboxState',
    'click .b-card-mod-disabled': 'disabledCard',
    'click .b-card--el-view': 'clickCard',
    'click .scroll-from': 'scrollTo',
    'click .b-side-menu--el-languages a': 'languageSwitcher',
    'click .pull-page': 'pullPage',
    'click .b-notification--el-close': 'closeNotification',
    'click .b-seller--el-show-contacts': 'sellerContacts'
  },

  presentationSlider: function() {
    var $slider = $('.b-presentation-slider');
    var $slide = $slider.find('.slide');
    var $controlsBlock = $slider.find('.b-presentation-slider--el-controls');
    var isMobile = window.innerWidth < 768;

    _.each($slide, function(el) {
      var $el = $(el);
      var $slide = $(el).find('img');
      $slide.attr('src', $el.data('src-' + (isMobile ? 'mobile' : 'desktop')));
    })

    if ($slider.data('controls') === 'dots' || isMobile) {
      $controlsBlock.addClass('dots');
      var controls = true;
    } else {
      $controlsBlock.addClass('images');
      var controls = 'thumbnails';
    }

    $slider.flexslider({
      animation: 'slide',
      controlNav: isMobile ? true : controls,
      directionNav: false,
      controlsContainer: $controlsBlock
    });
  },

  slideDirections: function(el) {
    el.preventDefault();

    var $el = $(el.currentTarget);
    var $slider = $el.parent();

    $el.hasClass('next') ? $slider.flexslider('next') : $slider.flexslider('prev');
  },

  toggleComments: function(el) {
    el.preventDefault();

    var $el = $(el.currentTarget);
    var $tabControl = $('.js-comment-tab-control');
    var $tabContent = $('.js-comment-tab-content');

    if ($tabControl.length > 1) {
	      $tabContent.toggle();
	      $tabControl.toggleClass('active');
    }
  },

  firstCommentBlock: function() {
//    $('.js-comment-tab-content').first().show();
  },

  addCommentFieldActivated: function(el) {
    var $el = $(el.currentTarget);

    $el.parent().addClass('active');
  },

  commentFileUploader: function() {
    var $commentFileUploader = $('.js-comment-file-uploader');
    var path = 'upload/';

    $commentFileUploader.fileupload({
      url: path,
      dataType: 'json',

      done: function (e, data) {
        $.each(data.result.files, function (index, file) {
          $('<div class="b-add-comment--el-add-file"><img src="'+ file.thumbnailUrl +'" alt="'+ file.name +'" /><a class="b-add-comment--el-del-file" href="#"></a><a class="b-add-comment--el-reload-file" href="#"></a></div>').appendTo('.js-files');
        });
      },

      progressall: function (e, data) {
        var progress = parseInt(data.loaded / data.total * 100, 10);
        $('.b-uploader-status .b-uploader-status--el-bar').css('width', progress + '%');
      }
    })
      .prop('disabled', !$.support.fileInput)
      .parent()
      .addClass($.support.fileInput ? undefined : 'disabled');
  },

  googleMap: function() {
    var $mapBlock = $('.b-google-map');
    if (!$mapBlock.length) { return; }
    var $mapTarget = $mapBlock.find('.b-google-map--el-map-target');
    var $viewOnMap = $mapBlock.find('.b-google-map--el-view-on-map');

    var mapAttr = $mapTarget.data('latlng').replace(/\s+/g, '').split(',');
    var pin = $mapTarget.data('pin');
    var LatLng = new google.maps.LatLng(mapAttr[0], mapAttr[1]);
    var zoomIndex = isNaN(parseInt($mapTarget.data('zoomindex'))) ? 17 : parseInt($mapTarget.data('zoomindex'));

    var mapOptions = {
      zoom: zoomIndex,
      center: LatLng,
      disableDefaultUI: true
    };

    var gMap = new google.maps.Map($mapTarget[0], mapOptions);

    var marker = new google.maps.Marker({
      position: LatLng,
      map: gMap,
      icon: '/sites/all/themes/qatarliving_v3/images/general/google-map-pin--' + pin + '.png',
      title: 'We Are Here!'
    });

    $viewOnMap.attr('href', 'https://www.google.com/maps/place/' + mapAttr);
  },

  openSideMenu: function(el) {
    el.preventDefault();
    var $el = $(el.currentTarget);
    var $sideMenu = $('.b-side-menu');
    var $root = $('.content-wrapper');
    var rootOffset = $root.offset();
    if($sideMenu.hasClass('notransition')){
    	$sideMenu.removeClass('notransition'); 	
    }
    var isMobile = window.innerWidth < 768;
    isMobile ? $sideMenu.css('left', 0) : $sideMenu.css('left', rootOffset.left + 15)
    $sideMenu.toggleClass('opened');
  },

  closeSideMenu: function(el) {
    el.preventDefault();
    var $el = $(el.currentTarget);
    var $sideMenu = $('.b-side-menu');
    $sideMenu.addClass('notransition'); 
    $sideMenu.toggleClass('opened');
    $sideMenu.css('left', 0);
  },

  languageSwitcher: function(el) {
    var $el = $(el.currentTarget);

    $el.hasClass('current') ? el.preventDefault() : false;
  },

  labelState: function() {
    var $optionBlock = $('.b-options-block');
    var $activeCheckboxes = $('.b-options-block--el-checkbox:checked');

    _.each($activeCheckboxes, function(el) {
      var $el = $(el);

      $el.parent().addClass('active');
    })
  },
  checkboxStateJobs: function(el){
	  	el.preventDefault();
	    var $el = $(el.currentTarget);
	    var $checkbox = $el.find('.b-options-block--el-checkbox');

	    $el.toggleClass('active');
	    $checkbox.prop('checked', $el.hasClass('active'));
  },
  checkboxState: function(el) {
    el.preventDefault();
    var $el = $(el.currentTarget);
    var $checkbox = $el.find('.b-options-block--el-checkbox');

    $el.toggleClass('active');
    $checkbox.prop('checked', $el.hasClass('active'));
  },
  clickCard: function(el){
  	el.preventDefault();
  	var $el = $(el.currentTarget).parent().parent();
  	var url = $el.attr('href');
  	if(url){
  		window.open(url, '_top');	
  	}
  },
  disabledCard: function(el) {
    //el.preventDefault();
  },

  scrollTo: function(el) {
    el.preventDefault();
    var $el = $(el.currentTarget);
    var $scrollTo = $($el.data('scroll-to'));

    $('html, body').animate({
      scrollTop: $scrollTo.offset().top
    }, 1000)
  },

  pullPage: function(el) {
    el.preventDefault();
    var $el = $(el.currentTarget);
    var pullPageURL = $el.data('pull-page-url') || '/';
    var iOS = bowser.ios;

    var $body = $('body').addClass('modal-active' + (iOS ? ' ios' : ''));
    var $modal = $('<div>').addClass('b-modal' + (iOS ? ' ios' : '')).appendTo($body);
    var $modalRoot = $('<div>').addClass('container-fluid content-wrapper').appendTo($modal);
    var $row = $('<div>').addClass('row').appendTo($modalRoot);
    var $col = $('<div>').addClass('col-sm-12').appendTo($row);
    var modalHeight = $modal.height();
    var modalContentHeight = (modalHeight * 95 / 100) - (iOS && window.pageYOffset > 0 ? 70 : 0);
    var modalContentShift = (modalHeight - modalHeight * 95 / 100) / 2;

    var $modalContent = $('<div>')
      .addClass('b-modal--el-content' + (iOS ? ' ios' : ''))
      .appendTo($col)
      .css({
        height: modalContentHeight,
        marginTop: modalContentShift
      });

    var $object = $('<object>')
      .addClass('b-modal--el-object')
      .appendTo($modalContent)
      .attr({
        data: pullPageURL,
        type: 'text/html'
      });

    var $modalClose = $('<a>')
      .addClass('b-modal--el-close');

    _.each([$modal, $modalClose], function(el) {
      $el = el;

      $el.on('click', function(el) {
        el.preventDefault();
        $modal.remove();
        $body.removeClass('modal-active' + (iOS ? ' ios' : ''));
      })
    });

    $object.on('load', function() {
      var _this = $(this);
      var $content = _this.contents();

      $modalClose.appendTo($content.find('.b-detailed-page-controls'));
      iOS ? (_this.css('height', $content.find('body').height())) : false;
    });
  },

  select: function() {
    var $select = $('.b-select-control');

    $select.styler({
      selectSearch: true
    });
  },

  closeNotification: function(el) {
    el.preventDefault();
    var $el = $(el.currentTarget);
    $el.parent().toggleClass('active');
  },

  sellerContacts: function(el) {
    el.preventDefault();
    var $el = $(el.currentTarget);
    var $contacts = $el.next();

    _.each([$el, $contacts], function(el) {
      $el = el;
      $el.slideToggle();
    });
  }
});

new Document();

var Window = Backbone.View.extend({
  el: window,

  initialize: function() {
    this.render();
  },

  render: function() {},

  events: {
    'load': function() {},

    'scroll': function() {
      this.stickyHeader();
    },

    'resize': function() {
      this.pullPage();
      this.stickyHeader();
    }
  },

  pullPage: function() {
    var $modal = $('.b-modal');
    var $modalContent = $('.b-modal--el-content');
    var modalHeight = $modal.height();
    var modalContentHeight = modalHeight * 95 / 100;
    var modalContentShift = (modalHeight - modalHeight * 95 / 100) / 2;

    $modalContent.css({
      height: modalContentHeight,
      marginTop: modalContentShift
    });
  },

  stickyHeader: function() {
    var windowWidth = this.el.innerWidth;
    var windowSY = this.el.scrollY;

    var $header = $('.b-header');
    var $scrollablePart = $header.find('.b-scrollable');
    var scrollablePartHeight = $scrollablePart.height();
    var headerTopHeight = $scrollablePart.parent().prev().height();


    if (windowWidth >= 768 && windowSY >= headerTopHeight) {
      $scrollablePart.addClass('scrolling');
      //$header.addClass('scrolling').css('padding-top', scrollablePartHeight);
    } else if (windowWidth >= 768) {
      $scrollablePart.removeClass('scrolling');
      $header.removeClass('scrolling').css('padding-top', 'unset');
    } else if (windowWidth < 768) {
      $scrollablePart.removeClass('scrolling');
      $header.removeClass('scrolling').css('padding-top', 'unset');
    }
  }
});

new Window();

var Popups = Backbone.View.extend({
    el: document,

    initialize: function() {

    },

    render: function() {

    },

    events: {
        'click .js-open-popup': 'openPopup',
        'click .js-close-popup': 'closePopup',
    },

    openPopup: function(ev) {
        ev.preventDefault();

        var $element = $(ev.currentTarget);
        var $type = $element.attr('data-type');

        $('#' + $type).css('display', 'block');
        $('body').addClass('b-body-open-popup');

        if($('.b-chat-popup-wrap').size() > 0){
            $('.b-chat-popup-wrap').animate({
                scrollTop: $(".b-chat-list").height()
            }, 0);
        }

    },

    closePopup: function(ev) {
        ev.preventDefault();

        $('.b-popup').css('display', '');
        $('body').removeClass('b-body-open-popup');
    }
});

new Popups();

